    "yeoman-generator": "^4.10.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-files": "^1.4.0",
    "ejs-lint": "^1.1.0",
    "eslint": "^7.0.0",
    "glob": "^7.1.6",
    "markdown-yaml-metadata-parser": "^2.0.5",
    "mocha": "^7.1.2",
    "nyc": "^15.0.1",
    "prettier": "2.0.5",
    "yeoman-test": "^2.6.0"
  }
